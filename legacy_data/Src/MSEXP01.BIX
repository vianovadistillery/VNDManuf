	CALL setup("EXPOSURE PANELS")
	btm$ = " F1-Exit  F6-Browse                                         F10-Change"
	COLOR 0, 3: LOCATE 25, 1: PRINT btm$; : COLOR 2, 0
	END IF
	acls% = 0

	IF NOT skip% THEN
		COLOR 2, 0
		LOCATE 3, 20: PRINT "Enter exposure number...> "
		LOCATE 3, 45: in$ = ""
		CALL inpnew(in$, 4!, updwn%, "13,59,64,66-68" + ALT$): GOSUB ALT
		END IF
		skip% = 0
	SELECT CASE updwn%
	CASE 59: CLOSE : CALL exitpgm(apgm$): CHAIN apgm$
	CASE 64:
			CALL browse71(s%)
			in$ = STR$(s%): updwn% = 13: skip% = -1: acls% = -1
	CASE 68: CALL change(rec%, updwn%)
	CASE 13
			IF LEN(in$) > 0 THEN
				rec% = VAL(in$)
			END IF
			alast% = LOF(71) / LEN(Exposure)
			IF LEN(in$) = 0 AND rec% < alast% THEN rec% = rec% + 1
			CALL dsply(rec%, updwn%)
	CASE ELSE
	END SELECT

	GOTO start

